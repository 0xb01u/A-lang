x = 4
y = 2
print "Valor de x al principio: " x
print "Valor de y al principio: " y

//Operaciones de bit
z = x << y
print "Valor de x tras la operacion shift left con y: " z
z = x & y
print "Valor de x tras la operacion and con y: " z
z = ~x
print "Valor de x tras la operacion not: " z

x = 40
y = 15.63
print "Nuevo valor de x: " x
print "Nuevo valor de y: " y

/* Operaciones
aritmeticas */
z = x * y

print "Valor del x * y: " z

x = x ^ 3 - (5 + 4) * 15 / 2
//63932.5
print "Nuevo valor de x = x ^ 3 - (5 + 4) * 15 / 2 = " x 
x = 45
print "Nuevo valor de x: " x
y = x % 9
print "El modulo de x % 9 es: " y

coseno = cos(x)
seno = sin(x)
tangente = tan(x)
logn = ln(x)
divi = x div 6

print "Coseno (radianes) de x: " coseno
print "Seno (radianes) de x: " seno
print "Tangente (radianes) de x: " tangente
print "Logaritmo neperiano de x: " logn
print "Cociente de x entre 6: " divi
z = ---x
print "Valor de x tras ser negado tres veces: " z
z2 = +++x
print "Valor de x tras aplicar tres veces el operador unario '+': " z2
